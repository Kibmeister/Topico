<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" type="text/css" media="screen" href="/style/stylesheet.css">
  <link rel="stylesheet" type="text/css" media="screen" href="/style/userstylesheets.css">
  <script src="js/formData.js"></script>
  <title>Wordpool</title>
</head>


<form id="id_myForm">
  <div id="id_formWordPool">
     <h3 id="id_formText">Upload main and helpwords to the cloud</h3>
    <div class="row">
      <div class="col-25">
        <label for="fname">Main Word</label>
      </div>
      <div class="col-75">
        <input type="text" id="id_mainWord" name="mainWord" placeholder="Main word..">
      </div>
    </div>
    <div class="row">
      <div class="col-25">
        <label for="helpWord1">First helpword</label>
      </div>
      <div class="col-75">
        <input type="text" id="id_helpWord1" name="helpWord1" placeholder="First helpword..">
      </div>
    </div>
    <div class="row">
      <div class="col-25">
        <label for="helpWord2">Second helpword</label>
      </div>
      <div class="col-75">
        <input type="text" id="id_helpWord2" name="helpWord2" placeholder="Second helpword..">
      </div>
    </div>
    <div class="row">
      <div class="col-25">
        <label for="helpWord3">Third helpword</label>
      </div>
      <div class="col-75">
        <input type="text" id="id_helpWord3" name="helpWord3" placeholder="Third helpword..">
      </div>
    </div>
    <button id="bt_wordpool" class="button" type="submit">Upload</button>
  </div>

</form>